{
  "name": "bbeg",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run write:env -s && ng build",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "deploy": "npm run write:env -s && ng deploy",
    "serve:sw": "npm run build -s && npx http-server -b -c 31536000 -S -C localhost.pem ./dist/bbeg/browser -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng test --configuration=ci",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "write:env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "dev:ssr": "ng run bbeg:serve-ssr",
    "serve:ssr": "node dist/bbeg/server/main.js",
    "build:ssr": "ng build && ng run bbeg:server",
    "prerender": "ng run bbeg:prerender"
  },
  "dependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular/animations": "~15.2.1",
    "@angular/cdk": "~14.2.0",
    "@angular/common": "~15.2.1",
    "@angular/compiler": "~15.2.1",
    "@angular/core": "~15.2.1",
    "@angular/fire": "^7.4.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "~15.2.1",
    "@angular/localize": "~15.2.1",
    "@angular/material": "~14.2.0",
    "@angular/platform-browser": "~15.2.1",
    "@angular/platform-browser-dynamic": "~15.2.1",
    "@angular/platform-server": "~15.2.1",
    "@angular/router": "~15.2.1",
    "@angular/service-worker": "~15.2.1",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ngneat/transloco": "^4.1.1",
    "@nguniversal/express-engine": "^14.0.2",
    "@triangular/rpg-awesome-icons": "^6.0.0",
    "express": "^4.15.2",
    "isbot": "^3.5.0",
    "localstorage-polyfill": "^1.0.1",
    "material-design-icons-iconfont": "^6.1.0",
    "ngx-quicklink": "^0.2.7",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular-eslint/builder": "~14.0.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/schematics": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.2.1",
    "@angular/compiler-cli": "~15.2.1",
    "@angular/language-service": "~15.2.1",
    "@ngneat/until-destroy": "^9.0.0",
    "@nguniversal/builders": "^15.2.0",
    "@ngx-rocket/scripts": "^5.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.20.37",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "htmlhint": "^1.0.0",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "postcss": "^8.4.5",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "~5.0.2",
    "stylelint-config-standard": "~24.0.0",
    "ts-node": "^10.1.0",
